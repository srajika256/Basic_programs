#include<bits/stdc++.h>
using namespace std;

long long int ncr(int n, int k)  
{  
    long long int res = 1;  
    if ( k > n - k )  
        k = n - k;  
    for (int i = 0; i < k; ++i)  
    {  
        res *= (n - i);  
        res /= (i + 1);  
    }  
  
    return res;  
}  

int main()
{
    int t;cin>>t;
    while(t--)
    {
        long int n,k,a,count1,count2,ans;
        vector<int> v,v1;
        cin>>n>>k;
        for(int i=0;i<n;i++)
        {cin>>a;
            v.push_back(a);
        }
        sort(v.begin(),v.end());
        //for(int i=0;i<k;i++)
        //{v1.push_back(v[i]);}
        
        count1 = count(v.begin(),v.begin()+ k,v[k-1]);
        count2 = count(v.begin(),v.end(),v[k-1]);
        //cout<<count1<<" "<<count2<<endl;
        ans = ncr(count2,count1);
        
        cout << ans <<endl;
        //cout<<ncr(6,1);
    }
    return 0;
}
